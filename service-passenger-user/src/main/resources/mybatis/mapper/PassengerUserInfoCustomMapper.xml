<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.online.taxi.dao.mapper.PassengerUserInfoCustomMapper" >


  <select id="selectByPhoneNumber" resultMap="com.online.taxi.dao.mapper.PassengerUserInfoMapper.BaseResultMap" parameterType="java.lang.String" >
    select
    <include refid="com.online.taxi.dao.mapper.PassengerUserInfoMapper.Base_Column_List" />
    from passenger_user_info
    where passenger_phone = #{passengerPhone}
  </select>


</mapper>